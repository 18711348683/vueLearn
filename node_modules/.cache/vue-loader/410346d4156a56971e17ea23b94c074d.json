e=\"name\"\r\n      :disabled=\"disabled\"\r\n      :trueLabel=\"trueLabel\"\r\n      :falseLabel=\"falseLabel\"\r\n      :checked=\"checked\"\r\n      @change=\"change\"\r\n    />\r\n  </label>\r\n</template>\r\n<script>\r\nexport default {\r\n  name: \"VcheckboxButton\",\r\n  props: {\r\n    label: {},\r\n    trueLabel: {},\r\n    falseLabel: {},\r\n    disabled: {\r\n      type: Boolean,\r\n      default: false\r\n    },\r\n    name: {\r\n      type: String,\r\n      default: \"checkbox\"\r\n    },\r\n    checked: {\r\n      type: Boolean,\r\n      default: false\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      data: this.$parent\r\n    };\r\n  },\r\n  methods: {\r\n    change: function() {\r\n      if (this.isChecked) {\r\n        this.$emit(\"checked\", this.label + \"/not\");\r\n      } else {\r\n        this.$emit(\"checked\", this.label);\r\n      }\r\n      this.$emit(\"input\", !this.isChecked);\r\n    }\r\n  },\r\n  computed: {\r\n    activeStyle: function() {\r\n      if (this.isChecked) {\r\n        return {\r\n          backgroundColor: this.data.fill,\r\n          color: this.data.textColor,\r\n          borderColor: this.data.fill\r\n        };\r\n      }\r\n      return \"\";\r\n    },\r\n    isChecked: function() {\r\n      let value = this.data.value;\r\n      for (let i = 0; i < value.length; i++) {\r\n        if (value[i] == this.label) {\r\n          return true;\r\n        }\r\n      }\r\n      return false;\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style scoped lang=\"stylus\">\r\n.button-checkbox {\r\n  position: relative;\r\n}\r\n\r\ninput {\r\n  position: absolute;\r\n  opacity: 0;\r\n  top: 0;\r\n  left: 0;\r\n}\r\n\r\n.button-span {\r\n  display: inline-block;\r\n  font-size: $fontSize;\r\n  padding: 12px 20px;\r\n  background: $mainBackgroundColor;\r\n  color: $mainTextColor;\r\n  border: 1px solid $mainBorderColor;\r\n  cursor: pointer;\r\n  border-left: 0;\r\n}\r\n\r\n.button-span:hover {\r\n  color: $mainColor;\r\n}\r\n\r\n.small